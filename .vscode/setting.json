{
  "editor.codeActionsOnSave": {
    "source.addMissingImports": true,
    "source.fixAll": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "files.associations": {
    ".*lintrc": "json",
    ".babelrc": "json",
    ".prettierrc": "json"
  },
  "files.exclude": {
    "node_modules": true
  }
}
