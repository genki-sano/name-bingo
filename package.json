    "lint": "next lint --dir src",
    "lint:fix": "yarn lint --fix",
    "lint:style": "stylelint src/**/*.{css,scss}",
    "lint:style:fix": "yarn lint:style --fix",
    "format": "prettier --write src/**/*"
